= link_to 'sign in', new_user_registration_path
= link_to 'logout', destroy_user_session_path, method: :delete
= link_to 'login', new_user_session_path

- if user_signed_in?
  = current_user.email
  = current_user.images

  = form_for @image do |f|
    = f.file_field :file
    = f.submit 'upload'


- @images.each do |image|
  = image_tag image.file.thumb.url
